<html>
    <head>
        <title>All cars</title>
    </head>
    <body>
        <ul>
            <% cars.forEach(car => { %>
                <li>
                    <%= car.make %>
                    <%= car.model %>,
                    <a href="/view-car/<%= car.id %>">view</a>
                    <a href="/edit-car/<%= car.id %>">edit</a>
                </li>
            <% }) %>
        </ul>
        <a href="/">Back to home</a>
    </body>
</html>